{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["react-app", "eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  
  "rules": {
    "quotes": ["error", "single"],
    "comma-dangle": ["error", "always-multiline"],
    "no-multi-spaces": "error",
    "react/react-in-jsx-scope": "off"  }
  
}
